{
  "compilerOptions": {
    "target": "ES2020",
    "module": "esnext",
    "strict": true,
    "esModuleInterop": false,
    "baseUrl": ".",
    "moduleResolution": "node",
    "importsNotUsedAsValues": "error",
    "sourceMap": true,
    "lib": [
      "ESNext",
      "DOM",
      "DOM.Iterable",
      "ScriptHost",
      "ESNext.Array"
    ]
  },
  "include": [
    "./**/*.ts",
    "./**/*.svelte",
  ],
  "exclude": [
    "node_modules"
  ]
}